<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="22dp"
        android:fontFamily="@font/b_mehr"
        android:text="تقویم"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/samad_btn"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cal_btn" />

    <ImageButton
        android:id="@+id/cal_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="291dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="7dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/notif_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_calender" />

    <ImageButton
        android:id="@+id/notif_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="291dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="7dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/news_btn"
        app:layout_constraintStart_toEndOf="@+id/cal_btn"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_calender"
        tools:srcCompat="@drawable/ic_notif" />

    <ImageButton
        android:id="@+id/samad_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="12dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/map_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/ic_calender"
        tools:srcCompat="@drawable/ic_samad" />

    <ImageButton
        android:id="@+id/news_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="291dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="7dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/notif_btn"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_calender"
        tools:srcCompat="@drawable/ic_news" />

    <ImageButton
        android:id="@+id/map_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="12dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/photo_btn"
        app:layout_constraintStart_toEndOf="@+id/samad_btn"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/ic_calender"
        tools:srcCompat="@drawable/ic_map" />

    <ImageButton
        android:id="@+id/photo_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="12dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/map_btn"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/ic_calender"
        tools:srcCompat="@drawable/ic_photos" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="22dp"
        android:fontFamily="@font/b_mehr"
        android:text="اطلاعیه ها"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/map_btn"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/notif_btn" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="22dp"
        android:fontFamily="@font/b_mehr"
        android:text="اخبار دانشگاه"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/photo_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/news_btn" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="149dp"
        android:fontFamily="@font/b_mehr"
        android:text="سامانه سماد"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/samad_btn" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="149dp"
        android:fontFamily="@font/b_mehr"
        android:text="نقشه"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView8"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/map_btn" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="149dp"
        android:fontFamily="@font/b_mehr"
        android:text="تصاویر دانشگاه"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/photo_btn" />

</androidx.constraintlayout.widget.ConstraintLayout>